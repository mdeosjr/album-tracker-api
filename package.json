      "@types/supertest": "^2.0.12",
      "jest": "^28.1.0",
      "nodemon": "^2.0.16",
      "prisma": "^3.14.0",
      "supertest": "^6.2.3",
      "ts-jest": "^28.0.2",
      "ts-node": "^10.7.0",
      "typescript": "^4.6.4"
   }
