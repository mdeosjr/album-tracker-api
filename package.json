      "bcrypt": "^5.0.1",
      "cors": "^2.8.5",
      "dotenv": "^16.0.1",
      "dotenv-cli": "^5.1.0",
      "express": "^4.18.1",
      "express-async-errors": "^3.1.1",
      "joi": "^17.6.0",
      "jsonwebtoken": "^8.5.1",
      "spotify-web-api-node": "^5.0.2"
   },
   "devDependencies": {
      "@faker-js/faker": "^6.3.1",
      "@types/bcrypt": "^5.0.0",
